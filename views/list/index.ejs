  <% for(let list of allist){ %>
  <div class="card col">
     <a href="/listing/<%= list._id %>">
       <img src="<%= list.image %>"  class="card-img-top" style="height:20rem" alt="listing_image" />
     </a>
    <div class="card-body">
      <p class="card-text">
      <b><%= list.title %> <br></b> 
        â‚¹<%= list.price.toLocaleString("en-IN") %>/night
      </p>
    </div>
  </div>
  <% } %>
  </div>
</body>
